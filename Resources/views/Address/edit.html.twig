{% extends "EkynaUserBundle::layout.html.twig" %}

{% block account %}
<h3>{{ 'ekyna_user.account.address.edit'|trans }}</h3>
<hr>
<form action="{{ path('ekyna_user_address_edit', {'addressId': address.id}) }}" {{ form_enctype(form) }} method="post" class="form-horizontal">
    <div class="form-body">
        {{ bootstrap_set_style('horizontal') }}
        {{ bootstrap_set_col_size('md') }}
        {{ form_widget(form) }}
    </div>
</form>
{% endblock %}